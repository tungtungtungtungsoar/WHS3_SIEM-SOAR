<!-- reverse shell agent 설정 참고-->

<group name="webshell,">
    <rule id="100510" level="10">
        <description>Webshell Reverse Shell Connection Detected</description>
        <match>webshell connections</match>
        <options>no_full_log</options>
    </rule>
</group>


