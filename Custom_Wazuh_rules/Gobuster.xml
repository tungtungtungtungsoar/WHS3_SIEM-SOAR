
<!-- 100420, 100421 -->
<group name="web,attack_attempt">
    <!--detected once-->
    <rule id="100420" level="5">
        <decoded_as>apache-user-agent</decoded_as>
        <field name="user_agent" type="pcre2">(?i)gobuster</field>
        <description>[Web Scan] Gobuster tool detected in access log</description>
        <mitre>
            <id>T1595.002</id>
        </mitre>
    </rule>
    <!--frequently
    detected-->
    <rule id="100421" level="12" frequency="10" timeframe="3">
        <if_matched_sid>100420</if_matched_sid>
        <description>[Web Scan] Gobuster activity repeatedly detected (10 times in 3s)</description>
        <mitre>
            <id>T1595.002</id>
        </mitre>
    </rule>
</group>